import static java.lang.System.*;

public class DoubleRunner
{
	public static void main(String[] args)
	{
		BiggestDouble run = new BiggestDouble(4.5,6.7,7.8,9.0); // "new" calls constructor
		System.out.println(run);                                // calls toString
		System.out.println("biggest = " + run.getBiggest() + "\n");

		//add more test cases by calling mutator method (not constructor) on run
		// ,then toString method, then getBiggest method as above
				
	}
}